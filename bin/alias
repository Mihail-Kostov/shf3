### module environment
alias mld="module load"
alias mlu="module unload"
alias mls="module list"
alias mla="module avail"
alias mdp="module display"

### shell framework
alias jobsub="jobmgr -s"
alias shfupd="shfmgr -u"
alias sshpub="sshmgr -s"
alias sshin="sshto -m"
alias sshmount="sshmnt -m"
alias sshumount="sshmnt -u"
