
function PUT() {
  import ssh
  local _h=${1:-default}
  shift
  local _s=${*}
  local i
  for i in ${_s} ; do
    if test -r "${i}" ; then
      ssh/put "${_h}" "${i}" 3
    fi
  done
}

function GET() {
  import ssh
  local _h=${1:-default}
  shift
  local _s=${*}
  local i
  for i in ${_s} ; do
    ssh/get "${_h}" "${i}" 3
  done
}
