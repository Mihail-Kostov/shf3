
function run/save() {
  import sys/dir

  if test -z "${WORKDIR}" || \
     test -z "${RESULT}"  || \
     test -z "${RESULTDIR}" ; then
    return ${_false_}
  fi

  # operator clean names
  local _wdir=$(ns/op/value "${WORKDIR}")
  local _sdir=$(ns/op/value "${RESULTDIR}")

  sys/dir/svwd "${_wdir}"

  local r
  local _r
  for r in ${RESULT}; do
    for _res in ${r} ; do
      sys/zip/zsvmv "${_wdir}/${_res}" "${_sdir}"
    done
  done

  return ${_true_}
}
