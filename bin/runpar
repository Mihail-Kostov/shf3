#!/bin/bash
source $(dirname ${BASH_SOURCE})/../lib/header

# TODO: sltpn
function help/runpar() {
  cat << EOF
Usage: ${gbn} queue:{omp,{mpiomp,mpi}/type}sockets:cores:gpu command
EOF
  exit 1
}

### args
if test $# -lt 2 ; then
  help/runarr
fi

if test $1 = "-h" || \
   test $1 = "help" ; then
  help/runpar
fi

_r=$1
shift
_x=$*
jobmgr -r ${_r} -p ${_x}
