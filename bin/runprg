#!/bin/bash
source $(dirname ${BASH_SOURCE})/../lib/header
import run
import gui

function help/runprg() {
  cat << EOF

Program wrapper

Usage: ${gbn} options
              -p program
              -g guide
              -s scheduler
              -i interactive
              -c level - check
              -v - verbose
EOF
  exit ${_false_}
}

### args
_p=""
_g=""
_s=""
_m="B"
_c=3
_v=false
while getopts hvip:g:s:c: o; do
  case "$o" in
    p) _p=$OPTARG;;
    g) _g=$OPTARG;;
    s) _s=$OPTARG;;
    c) _c=$OPTARG;;
    i) _m="I";;
    v) gdbg=true;;
    h) help/runprg;;
    *) help/runprg;;
  esac
done

### main
echo
msg $(date)
gui/line

run/prg "${_m}" ${_c} "${_p}" "${_g}" "${_s}"
